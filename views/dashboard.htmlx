<html lang="en">
	<head>
		%{indent(BOILERPLATE,levels=2)}
		<link rel="stylesheet" href="/assets/css/cards.css" />
	</head>
	<body>
		<div class="cards">
			<div class="card">
				<div>
					<canvas id="tdsChart"></canvas>
				</div>

				<script src="https://cdn.jsdelivr.net/npm/chart.js"></script>

				<script>
					const tds = document.getElementById("tdsChart");

					new Chart(tds, {
						type: "line",
						data: {
							labels: ["Red", "Blue", "Yellow", "Green", "Purple", "Orange"],
							datasets: [
								{
									label: "Total Disolved Solids",
									data: [12, 19, 3, 5, 2, 3],
									borderWidth: 1,
								},
							],
						},
						options: {
							scales: {
								y: {
									beginAtZero: true,
								},
							},
						},
					});
				</script>
			</div>
			<div class="card">
				<div>
